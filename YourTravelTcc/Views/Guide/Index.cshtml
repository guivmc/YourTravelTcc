@model IEnumerable<YourTravelTcc.Models.Message>
@{
    ViewData["Title"] = "Guide HomePage";
    Layout = "~/Views/Shared/_HomePageLayout.cshtml";
}

<h1>Messages:</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor( model => model.Text )
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach( var item in Model )
        {
            <tr>
                <td>
                    @Html.DisplayFor( modelItem => item.Text )
                </td>
                <td>
                    @Html.DisplayFor( modelItem => item.Text )
                </td>
            </tr>
        }
    </tbody>
</table>
